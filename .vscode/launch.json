{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/.bin/gulp",
			"stopOnEntry": false,
			"args": [
				"test"
			],
			"cwd": "${workspaceRoot}",
			"preLaunchTask": null,
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
			],
			"env": {
				"NODE_ENV": "development"
			},
			"externalConsole": false,
			"sourceMaps": false,
			"outDir": null
		},
		{
			"name": "Unit tests (local)",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/node_modules/.bin/_mocha",
			"stopOnEntry": false,
			"cwd": "${workspaceRoot}",
			"args": [
				"--no-timeouts",
				"--require", "pipeline/scripts/test-setup.js",
				"pipeline/scripts/**/*.tests.js"
			],
			"runtimeExecutable": null
		},
		{
			"name": "Unit tests (docker)",
			"type": "node",
			"request": "attach",
			"port": 5858,
			"preLaunchTask": "runMochaInDocker",
			"address": "localhost",
			"restart": false,
			"sourceMaps": false,
			"outDir": null,
			"localRoot": "${workspaceRoot}/",
			"remoteRoot": "/home/mist3/api/"
		}
	]
}
